<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CompassView">
        <attr name="azimuth" format="integer" />
    </declare-styleable>

    <declare-styleable name="CurvedProgressBar">
        <attr name="borderSize" format="dimension" />
        <attr name="dotWidth" format="dimension" />
        <attr name="dotHeight" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="textSizeProgress" format="dimension" />
        <attr name="textFontProgress" format="reference" />
        <attr name="textColorProgress" format="color" />
        <attr name="colorDotBackground" format="color" />
        <attr name="colorDotProgress" format="color" />
    </declare-styleable>

    <declare-styleable name="TextViewGradient">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textFont" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="radiusCorner" format="dimension" />
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ToggleImageView">
        <attr name="checkedImage" format="reference" />
        <attr name="uncheckedImage" format="reference" />
    </declare-styleable>
</resources>
